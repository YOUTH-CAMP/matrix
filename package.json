    "react-router-dom": "^5.2.0",
    "tailwindcss": "^2.2.7"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-grid-layout": "^1.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
